set nocompatible
filetype off

set rtp+=/usr/lib/python3.5/site-packages/powerline/bindings/vim/
set rtp+=~/.vim/bundle/Vundle.vim
filetype plugin indent on    
set tabstop=4
set shiftwidth=4
set expandtab

" Always show statusline
set laststatus=2

set t_Co=256

set nu
syntax on
colorscheme desert
